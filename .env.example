# ZenML Enterprise MLOps - Environment Configuration
# Copy this file to .env and fill in your credentials
# Usage: source scripts/use-workspace.sh <workspace>

# =============================================================================
# GCP Configuration (shared across all workspaces)
# =============================================================================
GCP_PROJECT_ID=zenml-core
GCP_REGION=us-central1

# =============================================================================
# ZenML Project (same name in both workspaces)
# =============================================================================
ZENML_PROJECT=cancer-detection

# =============================================================================
# ZenML Stack Names
# =============================================================================
ZENML_DEV_STACK=dev-stack
ZENML_STAGING_STACK=staging-stack
ZENML_PRODUCTION_STACK=gcp-stack

# =============================================================================
# ZenML Dev-Staging Workspace (enterprise-dev-staging)
# Used for: dev-stack, staging-stack
# =============================================================================
ZENML_DEV_STAGING_URL=https://xxxxxxxx-zenml.cloudinfra.zenml.io
ZENML_DEV_STAGING_API_KEY=ZENPROKEY_xxxxx

# =============================================================================
# ZenML Production Workspace (enterprise-production)
# Used for: gcp-stack (production)
# =============================================================================
ZENML_PRODUCTION_URL=https://yyyyyyyy-zenml.cloudinfra.zenml.io
ZENML_PRODUCTION_API_KEY=ZENPROKEY_yyyyy

# =============================================================================
# Optional: Model Exchange Bucket
# Set this after deploying shared/gcp terraform
# =============================================================================
# MODEL_EXCHANGE_BUCKET=zenml-core-model-exchange-xxxxxxxx
